---
const technologies = [
	'Go',
	'TypeScript',
	'Node.js',
	'Ruby on Rails',
	'SQL',
	'Postgres',
	'HTML/CSS',
	'React',
	'AWS/GCP',
	'Docker',
]
---

<div class="flex gap-2">
	{
		technologies.map((tech) => (
			<span class="inline-flex items-center rounded-md bg-gray-50 px-2 py-1 text-xs font-medium text-gray-600 ring-1 ring-inset ring-gray-500/10">
				{tech}
			</span>
		))
	}
</div>
